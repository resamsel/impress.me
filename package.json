{
  "name": "impress.me",
  "description": "Create impress.js presentations from markdown documents with style",
  "version": "0.3.0",
  "author": "RenÃ© Panzar",
  "bin": {
    "impress.me": "./bin/run"
  },
  "bugs": "https://github.com/resamsel/impress.me/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "clean-css": "^4.1.11",
    "highlight.js": "^8.9.1",
    "impress.js": "^1.0.0",
    "lodash": "4.17.15",
    "loglevel": "1.6.7",
    "marked": "^0.8.2",
    "pug": "2.0.4",
    "tslib": "^1.11.1",
    "uglify-es": "3.3.9"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/chai": "^4.2.11",
    "@types/clean-css": "4.2.1",
    "@types/highlight.js": "9.12.3",
    "@types/loglevel": "1.6.3",
    "@types/marked": "^0.7.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.18",
    "@types/pug": "2.0.4",
    "@types/uglify-es": "3.0.0",
    "@types/uglify-js": "3.0.5",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "material-colors": "1.2.6",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/templates",
    "/css"
  ],
  "homepage": "https://github.com/resamsel/impress.me",
  "keywords": [
    "impress.js",
    "markdown",
    "presentation",
    "html"
  ],
  "license": "ISC",
  "main": "src/index.ts",
  "oclif": {
    "bin": "impress.me"
  },
  "repository": "resamsel/impress.me",
  "scripts": {
    "build": "bin/run HelloWorld.md",
    "build:demo": "bin/run Demo.md --primary=blue --secondary=orange --strategy=planet --shape=circle --debug",
    "build:examples": "for i in examples/*.md; do bin/run $i --primary=blue --secondary=orange --strategy=planet --shape=circle; done",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
